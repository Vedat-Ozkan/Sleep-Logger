{
  "expo": {
    "name": "ChronoTrack",
    "slug": "chronotrack",
    "version": "1.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "scheme": "chronotrack",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    
    "android": {
      "package": "com.yourname.chronotrack",
      "permissions": [],                // keep minimal; add if needed
      "adaptiveIcon": {
        "backgroundColor": "#0a0f1a"
        
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router", "expo-sqlite"],
    "extra": {
      "googleAndroidClientId": "751794512635-u6g2u1lss4s7g1d99k02qfc0a3h0gst4.apps.googleusercontent.com"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
